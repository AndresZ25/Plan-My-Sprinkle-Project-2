<h1>Baby Shower Admin Dashboard</h1>
<h2>Guest List</h2>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Attendees</th>
      <th>Dietary Preferences</th>
    </tr>
  </thead>
  <tbody>
    {{#each guests}}
    <tr>
      <td>{{this.name}}</td>
      <td>{{this.email}}</td>
      <td>{{this.attendees}}</td>
      <td>{{this.dietaryPreferences}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
